#include <iostream>
#include <fstream>

using namespace std;

void openFiles (ifstream& infile, ofstream& outfile);

int main()
{

return 0;
};


void openFiles (ifstream& infile, ofstream& outfile)
{
char inFileName[40];
char outFileName[40];
cout<<"Enter the input file name: ";
cin>>inFileName;
infile.open(inFileName); //open input file
cout<<"Enter the output file name: ";
cin>> outFileName;
outfile.open(outFileName);//open out putfile
}
